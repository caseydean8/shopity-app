    body

        #list-builder
            #popup-box 
                img#popup-close(src='assets/images/close.png')
                #popup-box-content 
                h1 Sign-in or Create Account
                p 
                    | Stay up to date on the latest in the shopify app, plus receive exclusive coupons by
                    | subscribing to our newsletter. 
                    br
                    br
                form#popup-form(action='https://email_list_service_api.com')
                    input(type='hidden' name='list' value='key_list_etc') 
                    input(type='text' id='username' placeholder='Email Address') 
                    = ' '
                    input(type='password' id='password' placeholder='Password')
                    br
                    button(type='button' id='signin') Sign in
                    br
                    hr
                    br
                    input(type='hidden' name='list' value='key_list_etc')
                    input(type='text' name='' placeholder='Full Name') 
                    = ' '
                    input(type='text' name='' placeholder='Email Address')
                    = ' '
                    input(type='password' name='' placeholder='Create Password')
                    br
                    button(type='button' id='create') Create Account
                
            

